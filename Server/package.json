{
  "name": "airwatch-backend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node -r dotenv/config src/index.ts",
    "ingest:nasa": "ts-node -r dotenv/config src/workers/nasa.ts",
    "ingest:openaq": "ts-node -r dotenv/config src/workers/openaq.ts",
    "ingest:global": "ts-node -r dotenv/config src/workers/global.ts",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "nodemon": "^3.1.4",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "schema": "src/db/schema.prisma"
  }
}
